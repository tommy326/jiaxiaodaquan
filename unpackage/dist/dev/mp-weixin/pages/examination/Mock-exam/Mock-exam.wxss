
.main_wrap {
	padding-top: 68rpx;
}
.user_photo {
	display: block;
	width: 140rpx;
	height: 140rpx;
	border-radius: 50%;
	margin: 0 auto;
	background-color: #a3a3a3;
}
.user_name {
	color: #333333;
	font-size: 36rpx;
	text-align: center;
	line-height: 1;
	margin-top: 24rpx;
}
.hits_text {
	color: #959595;
	font-size: 28rpx;
	text-align: center;
	line-height: 1;
	margin-top: 32rpx;
}
.list {
	display: block;
	width: 100%;
	padding: 0 80rpx;
	box-sizing: border-box;
	margin-top: 64rpx;
}
.list .item {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	font-size: 30rpx;
	color: #333333;
	width: 100%;
	height: 52rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.list .item .text {
	-webkit-box-flex: 1;
	-webkit-flex: 1 1 auto;
	        flex: 1 1 auto;
	color: #959595;
	margin-left: 42rpx;
}
.Warm_prompt {
	display: block;
	width: 100%;
	padding: 0 80rpx;
	box-sizing: border-box;
	font-size: 24rpx;
	line-height: 34rpx;
	letter-spacing: 0px;
	color: #b3b3b3;
	margin-top: 32rpx;
}
.btn {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 590rpx;
	height: 90rpx;
	background-color: #3860ff;
	border-radius: 45rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	color: #ffffff;
	font-size: 34rpx;
	margin: 60rpx auto;
}

