.status_bar {
	display: block;
	height: var(--status-bar-height);
	width: 100%;
	background-color: #ffffff;
}
.top_view {
	height: var(--status-bar-height);
	width: 100%;
	position: fixed;
	background-color: #ffffff;
	top: 0;
	z-index: 999;
}

