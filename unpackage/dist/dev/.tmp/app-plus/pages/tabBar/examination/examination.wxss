
.tabs_box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 80rpx;
	padding: 0 32rpx;
	box-sizing: border-box;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.tabs_box .item {
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 auto;
	        flex: 0 0 auto;
	width: 92rpx;
	height: 80rpx;
	color: #909090;
	font-size: 36rpx;
	margin-right: 60rpx;
	text-align: center;
	line-height: 80rpx;
}
.tabs_box .active {
	color: #3860ff;
	position: relative;
	font-weight: bold;
}
.tabs_box .active:after {
	content: '';
	display: block;
	width: 32rpx;
	height: 6rpx;
	background-color: #3860ff;
	border-radius: 3rpx;
	position: absolute;
	left: 50%;
	margin-left: -16rpx;
	bottom: 2rpx;
}
.banner_box {
