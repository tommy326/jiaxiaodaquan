
.main_wrap {
	display: block;
	width: 100%;
	
	
	height: calc(100vh - 120rpx);
	
}
.swiper_wrap,
.scroll_box {
	display: block;
	width: 100%;
	height: 100%;
}
.scroll_box .pic {
	margin: 30rpx auto;
}
.footer_box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 120rpx;
	box-shadow: 0px -12rpx 40rpx 0 rgba(0, 0, 0, 0.04);
	border-radius: 40rpx 40rpx 0 0;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.footer_box .item {
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 auto;
	        flex: 0 0 auto;
	width: 25%;
	height: 120rpx;
	position: relative;
	color: #929292;
}
.footer_box .prev {
	background: url(data:image/png;base64,) no-repeat top 27rpx center;
	background-size: 20rpx 40rpx;
}
.footer_box .next {
	background: url(data:image/png;base64,) no-repeat top 27rpx center;
	background-size: 20rpx 40rpx;
}
.footer_box .collect_off {
	background: url(data:image/png;base64,) no-repeat top 21rpx center;
	background-size: 50rpx 46rpx;
}
.footer_box .collect_on {
	background: url(data:image/png;base64,) no-repeat top 21rpx center;
	background-size: 50rpx 46rpx;
	color: #fe8c00;
}
.footer_box .answer {
	background: url(data:image/png;base64,) no-repeat top 21rpx center;
	background-size: 46rpx 46rpx;
}
.footer_box .scantron {
	background: url(data:image/png;base64,) no-repeat top 21rpx center;
	background-size: 40rpx 46rpx;
}
.footer_box .item .text {
	display: block;
	width: 100%;
	position: absolute;
	left: 0;
	bottom: 16rpx;
	font-size: 28rpx;
	line-height: 1;
	text-align: center;
}
.main_wrap .sub_title {
	display: block;
	width: 100%;
	padding: 0 32rpx;
	box-sizing: border-box;
	line-height: 50rpx;
	margin: 12rpx 0 30rpx;
}
.main_wrap .sub_title .title_type {
	display: inline-block;
	width: 68rpx;
	height: 36rpx;
	background-color: #3860ff;
	border-radius: 18rpx 18rpx 4rpx 18rpx;
	font-size: 20rpx;
	color: #ffffff;
	text-align: center;
	line-height: 36rpx;
	margin-right: 20rpx;
}
.main_wrap .sub_title .title_text {
	font-size: 34rpx;
	color: #333333;
}
.main_wrap .radio_list {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 22rpx 32rpx;
	box-sizing: border-box;
	color: #333333;
	font-size: 34rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.main_wrap .radio_list .radio_icon {
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 auto;
	        flex: 0 0 auto;
	width: 56rpx;
	height: 56rpx;
	line-height: 56rpx;
	text-align: center;
	margin-right: 20rpx;
	border: solid 1px #3860ff;
	font-size: 32rpx;
	color: #3860ff;
	border-radius: 50%;
}
.main_wrap .radio_list .radio_icon_on {
	background-color: #3860ff;
	color: #ffffff;
}
.main_wrap .radio_list .radio_icon_off {
	background-color: red;
	color: #ffffff;
	border: solid 1px red;
}
.main_wrap .radio_list .radio_text {
	-webkit-box-flex: 1;
	-webkit-flex: 1 1 auto;
	        flex: 1 1 auto;
	line-height: 60rpx;
}
.baffle_wrap {
	display: none;
	width: 100%;
	
	
	height: 100%;
	
	position: fixed;
	left: 0;
	bottom: 0;
	background: rgba(0, 0, 0, 0.5);
	opacity: 0;
	transition: opacity 1s, display 1s;
	-webkit-transition: opacity 1s, display 1s;
}
.baffle_wrap_open {
	display: block;
	opacity: 1;
}
.Close_Answer {
	display: block;
	width: 100%;
	height: 120rpx;
}
.Answer_card {
	display: block;
	width: 100%;
	height: calc(100% - 120rpx);
	background: #ffffff;
	position: absolute;
	left: 0;
	top: 100%;
	border-radius: 40rpx 40rpx 0 0;
	transition: top 2s;
	-webkit-transition: top 2s;
}
.Answer_card_open {
	top: 120rpx;
}
.Answer_card .oper_list {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 132rpx;
}
.Answer_card .oper_list .item {
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 auto;
	        flex: 0 0 auto;
	width: 20%;
	position: relative;
	color: #929292;
}
.Answer_card .oper_list .item .text {
	display: block;
	width: 100%;
	line-height: 1;
	text-align: center;
	font-size: 20rpx;
	position: absolute;
	bottom: 27rpx;
	left: 0;
}
.Answer_card .oper_list .yes {
	background: url(data:image/png;base64,) no-repeat top 34rpx center;
	background-size: 32rpx 32rpx;
}
.Answer_card .oper_list .yes .text {
	color: #3860ff;
	font-size: 30rpx;
	bottom: 31rpx;
}
.Answer_card .oper_list .wrong {
	background: url(data:image/png;base64,) no-repeat top 34rpx center;
	background-size: 32rpx 32rpx;
}
.Answer_card .oper_list .wrong .text {
	font-size: 30rpx;
	bottom: 31rpx;
}
.Answer_card .oper_list .card {
	background: url(data:image/png;base64,) no-repeat top 28rpx center;
	background-size: 40rpx 46rpx;
}
.Answer_card .oper_list .collect_off {
	background: url(data:image/png;base64,) no-repeat top 28rpx center;
	background-size: 50rpx 46rpx;
}
.Answer_card .oper_list .collect_on {
	background: url(data:image/png;base64,) no-repeat top 28rpx center;
	background-size: 50rpx 46rpx;
	color: #fe8c00;
}
.Answer_card .oper_list .empty {
	background: url(data:image/png;base64,) no-repeat top 28rpx center;
	background-size: 42rpx 46rpx;
}
.Answer_card .opt_wrap {
	display: block;
	width: 100%;
	height: calc(100% - 132rpx);
}
.Answer_card .opt_wrap .sub_title {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 80rpx;
	background-color: #f6f6f6;
	color: #333333;
	font-size: 28rpx;
	text-indent: 33rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.Answer_card .opt_wrap_list {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	padding: 15rpx;
	box-sizing: border-box;
}
.Answer_card .opt_wrap_list .item {
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 auto;
	        flex: 0 0 auto;
	width: 92rpx;
	height: 92rpx;
	border: solid 1px #d9d9d9;
	border-radius: 50%;
	line-height: 92rpx;
	text-align: center;
	margin: 16rpx 12rpx;
	color: #929292;
	font-size: 28rpx;
}
.Answer_card .opt_wrap_list .on {
	background: rgba(235, 239, 255);
	color: #3860ff;
	border-color: rgba(235, 239, 255);
}
.Answer_card .opt_wrap_list .off {
	background: rgba(255, 238, 237);
	color: #ff564e;
	border-color: rgba(255, 238, 237);
}
.show {
	display: block;
	margin: 30rpx auto;
}
.hide {
	display: none;
	margin: 30rpx auto;
}
.checkbox_btn {
	width: 40%;
	margin-top: 50rpx;
	line-height: 2;
	background-color: #3860ff;
	color: #ffffff;
}

