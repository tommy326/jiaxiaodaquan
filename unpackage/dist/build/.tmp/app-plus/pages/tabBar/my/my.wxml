<view><view class="status_bar"><view class="top_view"></view></view><view class="top_line"></view><view class="user_box"><image class="user_photo" src="{{userAvatar}}" mode data-event-opts="{{[['tap',[['tapLogin',['$event']]]]]}}" bindtap="__e"></image><view class="user_info"><view data-event-opts="{{[['tap',[['tapLogin',['$event']]]]]}}" class="user_name" bindtap="__e">{{''+userName+''}}</view><text class="user_tag">开通会员</text></view></view><view class="column_list"><block wx:for="{{columnData}}" wx:for-item="item" wx:for-index="idx" wx:key="idx"><block><view class="item" data-idx="{{idx}}" data-event-opts="{{[['tap',[['tapNav',['$event']]]]]}}" bindtap="__e"><image class="img" src="{{'../../../static/images/mine/'+item.src+'.png'}}" mode></image><text class="text">{{item.title}}</text></view></block></block></view><view class="line"></view><view class="setting_list set_one"><block wx:for="{{setData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index<3}}"><block><view class="item" data-idx="{{index}}" data-event-opts="{{[['tap',[['tapJump',['$event']]]]]}}" bindtap="__e"><view class="photo_wrap"></view><view class="text">{{''+item+''}}</view></view></block></block></block></view><view class="line"></view><view class="setting_list set_two"><block wx:for="{{setData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index>2}}"><block><view class="item" data-idx="{{index}}" data-event-opts="{{[['tap',[['tapJump',['$event']]]]]}}" bindtap="__e"><view class="photo_wrap"></view><view class="text">{{''+item+''}}</view></view></block></block></block></view></view>