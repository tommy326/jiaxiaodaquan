<view class="main_container"><view class="list"><view class="item line"><view class="key">头像</view><image class="pic" src="{{userAvatar}}" mode></image></view><view data-event-opts="{{[['tap',[['changeName',['$event']]]]]}}" class="item" bindtap="__e"><view class="key">用户名</view><view class="val">{{''+userName+''}}</view></view></view><view class="list"><view data-event-opts="{{[['tap',[['changeTel',['$event']]]]]}}" class="item line" bindtap="__e"><view class="key">手机号</view><view class="val">{{''+userTel+''}}</view></view><view data-event-opts="{{[['tap',[['changePass',['$event']]]]]}}" class="item" bindtap="__e"><view class="key">修改密码</view></view></view><view class="list"><view data-event-opts="{{[['tap',[['tapAbout',['$event']]]]]}}" class="item" bindtap="__e"><view class="key">关于我们</view></view></view><view data-event-opts="{{[['tap',[['SignOut',['$event']]]]]}}" class="Sign_out" bindtap="__e">退出登录</view></view>